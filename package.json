{
  "name": "remark-embed-images",
  "version": "1.0.0-pre.0",
  "description": "Embed images with data: URIs, inlining base64-encoded sources",
  "license": "MIT",
  "keywords": [
    "image",
    "embed",
    "base64",
    "inline",
    "remark"
  ],
  "repository": "https://github.com/dherges/remark-embed-images",
  "bugs": "https://github.com/dherges/remark-embed-images/issues",
  "author": "David Herges <david@spektrakel.de>",
  "files": [
    "index.js"
  ],
  "dependencies": {
    "datauri": "^1.0.5",
    "prettier": "^1.14.2",
    "unist-util-visit": "^1.0.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "nyc": "^12.0.0",
    "remark": "^9.0.0",
    "remark-cli": "^5.0.0",
    "remark-html": "^8.0.0",
    "remark-preset-wooorm": "^4.0.0",
    "standard-version": "^4.2.0",
    "tape": "^4.0.0",
    "to-vfile": "^5.0.0",
    "validate-commit-msg": "^2.12.2",
    "xo": "^0.22.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write \"**/*.js\" && xo --fix",
    "test:api": "node test",
    "test:coverage": "nyc --reporter lcov tape test.js",
    "test": "yarn run format && yarn run test:coverage",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version --prerelease pre --message 'release: cut v%s'"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  }
}
